{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "desktop",
  "version": "0.1.0",
  "identifier": "com.apple.desktop",
  "build": {
    "frontendDist": "../src",
    "devUrl": "http://localhost:8000"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "desktop",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "externalBin": [],
    "resources": [
      "python_backend"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}